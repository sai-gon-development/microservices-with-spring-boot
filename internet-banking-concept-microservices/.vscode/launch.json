  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Config Server",
      "request": "launch",
      "mainClass": "com.saigon.development.internetbankingconfigserver.application.InternetBankingConfigServerApplication"
    },
    {
      "type": "java",
      "name": "Launch Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Gateway",
      "request": "launch",
      "mainClass": "com.saigon.development.internetbankingapigateway.application.InternetBankingApiGatewayApplication",
      "projectName": "internet-banking-api-gateway"
    },
    {
      "type": "java",
      "name": "Registry",
      "request": "launch",
      "mainClass": "com.saigon.development.internetbankingserviceregistry.application.InternetBankingServiceRegistryApplication",
      "projectName": "internet-banking-service-registry"
    },
    {
      "type": "java",
      "name": "Fund Transfer",
      "request": "launch",
      "mainClass": "com.saigon.development.internetbankingfundtransferservice.application.InternetBankingFundTransferServiceApplication",
      "projectName": "internet-banking-fund-transfer-service"
    },
    {
      "type": "java",
      "name": "User",
      "request": "launch",
      "mainClass": "com.saigon.development.internetbankinguserservice.application.InternetBankingUserServiceApplication",
      "projectName": "internet-banking-user-service"
    }
  ]
}